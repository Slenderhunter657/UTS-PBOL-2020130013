<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projecpbol.InputPlayerController">
   <children>
      <Label layoutX="219.0" layoutY="14.0" text="Input Inventory">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label layoutX="159.0" layoutY="71.0" text="Player ID:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="txtplayerid" layoutX="269.0" layoutY="70.0">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Label layoutX="159.0" layoutY="120.0" text="Player Name:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="txtplayername" layoutX="269.0" layoutY="120.0">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Label layoutX="159.0" layoutY="170.0" text="Reset Count:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="txtresetcount" layoutX="269.0" layoutY="170.0">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Button fx:id="btnsimpan" layoutX="390.0" layoutY="314.0" mnemonicParsing="false" onAction="#simpanKlik" text="Submit" />
      <Button fx:id="btnbatal" layoutX="320.0" layoutY="314.0" mnemonicParsing="false" onAction="#batalKlik" prefHeight="25.0" prefWidth="54.0" text="Clear" />
      <Button fx:id="btnkeluar" layoutX="320.0" layoutY="354.0" mnemonicParsing="false" onAction="#keluarKlik" prefHeight="25.0" prefWidth="54.0" text="Exit" />
   </children>
</AnchorPane>
